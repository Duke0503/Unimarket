nav.navbar.navbar-expand-lg.navbar-light.bg-light
  button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarNavDropdown', aria-controls='navbarNavDropdown', aria-expanded='false', aria-label='Toggle navigation')
    span.navbar-toggler-icon
  #navbarNavDropdown.collapse.navbar-collapse
    ul.navbar-nav
      li
        a.nav-link(href=`${prefixAdmin}/dashboard`)
          | DashBoard 
          span.sr-only (current)
      if(role.permissions.includes("products-category_view"))
        li
          a.nav-link(href=`${prefixAdmin}/products-category`) Danh mục sản phẩm
          
      if(role.permissions.includes("products-category_view"))
        li
          a.nav-link(href=`${prefixAdmin}/products`) Danh sách sản phẩm

      if(role.permissions.includes("accounts_view"))
        li
          a.nav-link(href=`${prefixAdmin}/accounts`) Danh sách tài khoản

      if(role.permissions.includes("roles_view"))
        li
          a.nav-link(href=`${prefixAdmin}/roles`) Nhóm quyền

      if(role.permissions.includes("roles_permissions"))
        li
          a.nav-link(href=`${prefixAdmin}/roles/permissions`) Phân quyền

      li
          a.nav-link(href=`${prefixAdmin}/settings/general`) Cài đặt chung

      
